#include <iostream> // Указатель This
#include <cstdlib>
using namespace std;

class DataFunction
{
 char Name[35];
int Age;


public:

DataFunction ( char Name[], int Age)
{ (this->Name, Name);
(this->Age = Age); // Возможно тут не так написал,не до конца понял смысл указателя в явном виде :(
}

void GetData()
{ 
cout << Name << "[ ]: " << Age << endl;
}
        };

int main()
{
setlocale(LC_ALL, "ru");

DataFunction a("Anton Trofimov", 18);

a.GetData();

return 0;
system("pause");
}
